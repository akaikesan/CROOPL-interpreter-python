class Program
    Fib f

    method main()
        new separate Fib f
        call f::init()
        call f::initSeparate()
        call f::fib()
        uncall f::fib()
        uncall f::initSeparate()


class Fib
    int[] xs
    Fib g
    
    method init()
        new int[2] xs

    method initSeparate()
        new separate Fib g
    
    method minus()
        xs[1] -= 100
  
    method fib()
        call g::init()
        call g::minus()

     method get (int out)
        out ^= xs[1]
