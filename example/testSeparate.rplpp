class Fib
    int[] xs
    Fib f
    int x
    
    method init()
        new int[2] xs

    method initSeparate()
        new separate Fib f
    
    method minus()
        xs[1] -= 100
  
    method fib()

        call f::init()
        call f::minus()

     method get (int out)
        out ^= xs[1]
